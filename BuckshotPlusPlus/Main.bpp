test = "Hi :D"

include "Test.bpp"

page Home{
	title = "My awesome home page"
	body = MyBodyView
}

page Hello{
	title = "My awesome Hello page!"
}

route Index{
	type = "get"
	path = "/"
	target = Home
}

route Test{
	type = "get"
	path = "/test"
	target = Hello
}

server Main{
	router = [Index,Test]
}
