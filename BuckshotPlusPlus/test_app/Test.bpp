view Button{
	type = "a"
	content = "My default button"
	background_color = "#9b59b6"
	font_size = "26px"
	padding = "5px"
	cursor = "pointer"
	color = "white"
	text_transform = "uppercase"
	border_padding = "5px"
}

view MyLink:Button{
	content = "The other page"
	href = "/dylan"
}

view WhiteTitle{
	color = "white"
	type = "h1"
	content = "Test text"
}

view MyName:WhiteTitle{
	content = "Welcome to my website!"
}

view MyBodyView{
	background_color = "black"
	type = "body"
	content = [MyName,MyLink]
}

view MyTestView:MyBodyView{
	content = "I'm a child body"
}

view MyOtherTest:MyTestView{
	backgroundColor = "blue"
}

